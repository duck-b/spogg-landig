  "scripts": {
    "start": "react-scripts start",
    "start:window": "set \"REACT_APP_DEPLOY_TYPE=development\" &&  react-scripts start",
    "start:window-prod": "set \"REACT_APP_DEPLOY_TYPE=production\" npm run start",
    "start:dev": "REACT_APP_DEPLOY_TYPE=development npm run start",
    "start:prod": "REACT_APP_DEPLOY_TYPE=production npm run start",
    "build": "react-scripts build",
    "build:window": "set \"REACT_APP_DEPLOY_TYPE=development\" && npm run build",
    "build:window-prod": "set \"REACT_APP_DEPLOY_TYPE=production\" npm run build",
    "build:dev": "REACT_APP_DEPLOY_TYPE=development npm run build",
    "build:prod": "REACT_APP_DEPLOY_TYPE=production npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
